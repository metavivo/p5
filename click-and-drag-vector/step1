

function setup() {
  createCanvas(700, 400);
  background(255);
  fill(0);    
}

function draw() {  
}

function mousePressed() {
    background(255);
    x1 = mouseX;
    y1 = mouseY;
} 


function mouseDragged() {
    x2 = mouseX;
    y2 = mouseY;
    background(255);
    line(x1, y1, x2, y2);
}
