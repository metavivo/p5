let xtop = 0;
let ytop = 0;
let l = 100;
let r = 10;

function setup() {
  
  createCanvas(600, 400);
  background(220);
  
  push();
  
  translate(width/2, height/2);
  
  angleMode(DEGREES);
  rotate(45);

  line(xtop ,ytop , xtop, ytop + l);
  ellipse(xtop, ytop + l + r/2, r, r);
  
  pop();

}


function draw() {
}
